{
   "name": "graphql-ts-server-boilerplate",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/connect-redis": "^0.0.8",
      "@types/express": "^4.16.0",
      "@types/express-session": "^1.15.11",
      "@types/ioredis": "^4.0.4",
      "@types/jest": "^23.3.10",
      "@types/node": "^10.12.12",
      "@types/node-fetch": "^2.1.4",
      "@types/request-promise": "^4.1.42",
      "@types/sparkpost": "^2.1.4",
      "@types/uuid": "^3.4.4",
      "@types/yup": "^0.26.13",
      "gql2ts": "^1.10.1",
      "jest": "^23.6.0",
      "nodemon": "^1.18.7",
      "start-server-and-test": "^1.7.11",
      "ts-jest": "^23.10.5",
      "ts-node": "6.0.0",
      "tslint": "^5.11.0",
      "tslint-config-prettier": "^1.17.0",
      "typescript": "3.2.1"
   },
   "dependencies": {
      "@gql2ts/from-schema": "^1.10.1",
      "axios": "^0.18.0",
      "bcryptjs": "^2.4.3",
      "connect-redis": "^3.4.0",
      "dotenv": "^6.2.0",
      "express": "^4.16.4",
      "express-session": "^1.15.6",
      "graphql": "^14.0.2",
      "graphql-import": "^0.7.1",
      "graphql-request": "^1.8.2",
      "graphql-tools": "^4.0.3",
      "graphql-yoga": "^1.16.7",
      "ioredis": "^4.2.0",
      "node-fetch": "^2.3.0",
      "pg": "^7.7.1",
      "reflect-metadata": "^0.1.12",
      "request-promise": "^4.2.2",
      "sparkpost": "^2.1.3",
      "typeorm": "0.2.9",
      "uuid": "^3.3.2",
      "yup": "^0.26.6"
   },
   "scripts": {
      "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
      "test-server": "NODE_ENV=test ts-node --files src/index.ts",
      "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
      "ci": "start-server-and-test test-server http://localhost:4000 test",
      "gen-schema-types": "ts-node src/scripts/createTypes.ts"
   },
   "jest": {
      "globalSetup": "./src/testSetup/callSetup.js",
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
